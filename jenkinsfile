    agent any
    stages {
        stage('Build') {
            steps {
              script{
                sh "This is from build stage"
              }
              
            }
        }
        stage('Test') {
            steps {
              script{
                sh "This is from Test stage"
              }
            }
        }
        stage('Deploy') {
            steps {
              script{
                sh "This is from deploy stage"
              }
            }
        }
    }
}